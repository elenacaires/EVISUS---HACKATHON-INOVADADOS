            on_release:app.root.current = 'profsaude'

<Gestor>:
    BoxLayout:
        orientation:'vertical'
        ActionBar:
            pos_hint: {'top':1}
            background_color: 15/255,97/255,170/255, 1
            background_image: ''
            ActionView:
                ActionPrevious:
                    app_icon: ''
                    title:'CANAL DO GESTOR'
                    on_release:app.root.current = 'menu'
        ScrollView:
            BoxLayout:
                orientation:'vertical'
                size_hint_y:None
                height:root.height
                padding:[30, 30, 30, 30]
                canvas:
                    Color:
                        rgba: 237/255.0,237/255.0,237/255.0,1
                    Rectangle:
                        size:self.size
                        pos:self.pos
                Button:
                    text: 'Recomendações (Pesquisa Saúde)'
                    background_normal: 'buttonbg.png'
                    color: 15/255,97/255,170/255
                    text_size: self.width * 0.9, self.height
                    halign: 'center'
                    valign: 'center'
                    on_release:app.root.current = 'recomend'
                Button:
                    text: 'Prioridades e indicadores em saúde'
                    background_normal: 'buttonbg.png'
                    color: 15/255,97/255,170/255
                    text_size: self.width * 0.9, self.height
                    halign: 'center'
                    valign: 'center'
                    on_release:app.root.current = 'indicadores'
                Button:
                    text: 'Produção ambulatorial e hospitalar'
                    background_normal: 'buttonbg.png'
                    color: 15/255,97/255,170/255
                    text_size: self.width * 0.9, self.height
