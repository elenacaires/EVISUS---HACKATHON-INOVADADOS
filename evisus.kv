<Menu>:
    BoxLayout:
        orientation:'vertical'
        padding:[100, 150, 100, 100]
        spacing:40
        canvas:
            Color:
                rgba:1,1,1,1
            Rectangle:
                size:self.size
                pos:self.pos
                source:'frame.png'
        Label:
            text: 'Você é:'
            font_size:18
            color: 43/255,114/255,184/255
        Button:
            text:'Gestor'
            background_normal: ''
            background_color: 43/255,114/255,184/255
            on_release:app.root.current = 'gestor'
        Button:
            text:'Pesquisador'
            background_normal: ''
            background_color: 43/255,114/255,184/255
            on_release:app.root.current = 'pesquisador'
